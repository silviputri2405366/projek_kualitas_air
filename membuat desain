  if (air->partikel == 0) skor += 0;
    else if (air->partikel == 1) skor += 1;
    else skor += 2;

 
    int kategori;
    if (skor <= 1) kategori = 1;
    else if (skor <= 3) kategori = 2;
    else if (skor <= 5) kategori = 3;
    else kategori = 4;

    switch (kategori) {
        case 1:
            return "Air bersih layak konsumsi";
        case 2:
            return "Air keruh masih dapat dikonsumsi";
        case 3:
            return "Air keruh tidak layak konsumsi";
        case 4:
            return "Air kotor tidak layak konsumsi";
        default:
            return "Kategori tidak diketahui";
    }
}
